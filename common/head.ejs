<!doctype html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
    <meta name="description" content="">
    <meta name="keywords" content="">

    <link rel="icon" type="image/vnd.microsoft.icon" href="/assets/favicon.ico">

    <title>FRONT SAMPLE<%= conf.title %></title>

    <%
    var l = filename.split('ejs/')[1].split('/').length;

    conf.rel = '';
    for(var i=0; i<l; i++) {
        conf.rel = '../' + conf.rel;
    }
    conf.name = filename.split('/')[filename.split('/').length - 1].replace('.ejs','');

    if (typeof conf.js === 'string') { conf.js = [conf.js]; }
    if (typeof conf.css === 'string') { conf.css = [conf.css]; }
    %>

    <% for(var j=0; j<conf.css.length; j++){ %>
    <link rel="stylesheet" href="/css/<%= conf.css[j] %>.css">
    <% } %>

</head>
